function Cfind() {
    var found = false;
    var board = '';
	var Writer = '';
	var cafemain = document.getElementById('cafe_main').contentWindow;
    if (cafemain.document.getElementsByClassName('head m-tcol-p').length == 1) found = true;
    var a = cafemain.document.getElementsByClassName('m-tcol-c b');
    for (var i = 0, leng = a.length; i < leng; i++) {
		if (a[i].tagName === 'A' && a[i].outerHTML.indexOf('ui(event') != -1){
			var outer = a[i].outerHTML.split('(')[1].split(')')[0].split(' ').join('').split(\"'\").join('').split(',');
			Writer = outer[1] + ',' + outer[3];
		}
    }
	a = cafemain.document.getElementsByTagName('a');
	for (var i = 0, leng = a.length; i < leng; i++) {
		if (a[i].outerHTML.indexOf('http://cafe.naver.com/ArticleList.nhn?search.clubid=') !== -1 && a[i].outerHTML.indexOf('targetChangeForMacSafari') !== -1) { 
            board = a[i].innerHTML;
        }
    }
    window.external.CallForm(found.toString(), board, Writer);
}
var a = document.getElementsByTagName('a'); for (var i = 0, leng = a.length; i < leng; i++) if (a[i].innerHTML == '쪽지보내기') a[i].click();